#include <iostream>
using namespace std;
int main() 
{
    int t;
    cin >> t;
    while (t--) 
    {
        int a, x, y;
        cin >> a >> x >> y;
        if ((x <= a && a <= y) || (y <= a && a <= x))
            cout << "NO"<<endl;
        else
            cout << "YES"<<endl;
    }
    return 0;
}
